{
  "name": "my-app",
  "productName": "electron-app",
  "version": "0.1.0",
  "description": "ElectronApp With GoLang",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^13.5.0",
    "astilectron": "^0.33.0",
    "axios": "^0.27.2",
    "bootstrap": "^5.1.3",
    "callback": "^0.0.1",
    "electron-installer-debian": "^3.1.0",
    "electron-is-dev": "^2.0.0",
    "fetch": "^1.1.0",
    "ipc-node-go": "^0.2.0",
    "lzip": "^1.0.10",
    "node-ipc": "^11.1.0",
    "react": "^18.1.0",
    "react-bootstrap": "^2.4.0",
    "react-dom": "^18.1.0",
    "react-fetch": "^0.0.9",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4",
    "websocket": "^1.0.34",
    "ws": "^8.8.0"
  },
  "main": "public/electron.js",
  "build": {
    "appId": "my-app",
    "linux": {
      "target": [
        "deb"
      ],
      "category": "Utility"
    },
    "files": [
      "node_modules",
      "public/electron.js"
    ],
    "extends": null
  },
  "homepage": "./",
  "scripts": {
    "start": "export BROWSER=none && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "dist:linux": "electron-builder --linux",
    "eject": "react-scripts eject",
    "dev": "concurrently -k \"BROWSER=none npm start\" \"npm:electron\" \"go run HelloWorld.go\" ",
    "electron": "wait-on tcp:3000 && electron .",
    "package-linux": "electron-packager . my-app --overwrite --asar=true --platform=linux --arch=x64 --prune=true --out=release-builds",
    "create-debian-installer": "electron-installer-debian --src release-builds/my-app-linux-x64/ --arch amd64 --config debian.json"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "author": "Nimrah <syedanimrah7420@gmail.com>",
  "devDependencies": {
    "concurrently": "^7.2.2",
    "electron": "^18.3.5",
    "electron-builder": "^23.1.0",
    "electron-is-dev": "^2.0.0",
    "electron-packager": "^15.5.1",
    "wait-on": "^6.0.1"
  }
}
